pipeline {
    agent any

    tools {
        maven 'Maven'// Replace with your Maven installation name in Jenkins
    
        jdk 'Java Home'
    }

    stages {
        stage('Build') {
            steps {
                bat 'mvn clean compile'
            }
        }
        stage('Test') {
            steps {
                bat 'mvn test'
            }
        }
        stage('Package') {
            steps {
                bat 'mvn package'
            }
        }
    }
}
